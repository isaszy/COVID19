<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.corona.COVID19">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="COVID19"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_resultadoscanner"></activity>
        <activity android:name=".Activity_scanner" />
        <activity android:name=".Activity_login" />
        <activity android:name=".Activity_cadastro" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>